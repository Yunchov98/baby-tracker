{
  "hosting": {
    "public": "./client/dist/client/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "database": {
      "rules": "database.rules.json"
    }
  }
}